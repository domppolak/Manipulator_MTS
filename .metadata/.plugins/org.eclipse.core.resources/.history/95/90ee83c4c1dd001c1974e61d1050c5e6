#include "fabrik.h"
#include <math.h>

float distance(const int p1[3], const int p2[3])
{
	int temp[3];

	temp[0]=p2[0]-p1[0];
	temp[1]=p2[1]-p1[1];
	temp[2]=p2[2]-p1[2];

	return abs(pow(pow(temp[0],2),pow(temp[1],2),pow(temp[2],2),1/3));
}

void newPositions(float p[3][3], float t[3])
{
	int root[3]={0,0,0};

	float d1=distance(root,p[0]);
	float d1max=380;
	float d2=distance(p[0],p[1]);
	float d3=distance(p[1],p[2]);
	float dist=distance(root,t);

	if(dist>d1max+d2+d3)
	{
		printf("Cel poza obszarem roboczym");
	}

	for (int i=0;i<3;i++)
	{
		int r;
		r=distance()
	}
}
